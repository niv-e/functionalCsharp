
@page
@model IndexModel
@using Models.Types.Components;

@{
    ViewData["Title"] = "Functional page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
    @* <component type="typeof(Components.BusinessMonths)" render-mode="Static" param-fortimestamp="@DateTime.UtcNow" /> *@

</div>

<table class="table table-striped">
    @{int ordinal = 0;}
    <tr><th>#</th><th>Name</th><th>SKU</th><th>Barcode</th></tr>
    @foreach (Part part in Model.AllParts.OrderBy(part => part.Name))
    {
        <tr>
            <th>@(++ordinal)</th>
            <td>@part.Name</td>
            <td>@part.Sku.Value</td>
            <td><img src="data:"></td>
        </tr>
    }
</table>